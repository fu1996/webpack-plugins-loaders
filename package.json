{
  "name": "webpack-plugins-loaders",
  "version": "1.0.0",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "publish": "lerna publish",
    "clean": "lerna clean -y && rm -rf node_modules && pnpm i",
    "prettier:check": "prettier --write --config .prettierrc.js --check .",
    "eslint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "tsc:fix": "tsc -p ./tsconfig.json",
    "commit": "cz",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "keywords": [],
  "author": "fujunkui",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@jest/globals": "^29.7.0",
    "@release-it-plugins/lerna-changelog": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.6.2",
    "@types/node-fetch": "^2.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lerna": "^7.1.5",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "rollup": "^3.29.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "loader-utils": "^3.2.1",
    "schema-utils": "^4.0.0"
  }
}
